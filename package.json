    "@storybook/test": "^8.2.7",
    "@types/node": "^20.14.13",
    "@types/uuid": "^10",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "storybook": "^8.2.7",
    "tslib": "^2.6.3",
